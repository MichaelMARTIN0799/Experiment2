#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
    int n, row;
    

    cout << "Enter a number: ";
    cin >> n;
    cout<<"--------------------------------------------------"<<endl;
    cout << "Multiplication table of " << n<<endl;
    
    for (row = 1; row <= 10; ++row) 
    {
       	int product = n * row;
        cout << n << " x " << row << " = " << product<<endl;
    }
    _getch();

    return 0;
}
